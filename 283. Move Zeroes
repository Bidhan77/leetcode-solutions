class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int left=0,right;
       for(right=0; right<nums.size();right++){
           if (nums[right]==0){
               continue;
           }
           else
           {
               swap(nums[right],nums[left]);
               left++;
           }
               
       } 
        return;
    }
};
